<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wanmait.exam.mapper.StudentAnswerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wanmait.exam.entity.StudentAnswer">
        <id column="id" property="id" />
        <result column="test_id" property="testId" />
        <result column="question_id" property="questionId" />
        <result column="student_id" property="studentId" />
        <result column="student_anseer" property="studentAnseer" />
        <result column="student_score" property="studentScore" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, test_id, question_id, student_id, student_anseer, student_score
    </sql>

</mapper>
